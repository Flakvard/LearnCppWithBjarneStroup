
#include "../std_lib_facilities.h"

int main()
{
    int s1 = area(7;   //error: ) missing
    int s2 = area(7)    //error: ; missing
    Int s3 = area(7);   //error: Int is not a type
    int s4 = area ('7); //error: non-terminated character ('missing)

    int x0 = arena(7); //error: undeclared function
    int x1 = area(7); //error: wrong number of arguments.
    int x2 = area("seven",2); //error: 1st argument has a wrong type
    int x3 area(7); //error missing an equal operator.
    int x4 area(7), //error: wrong define statement.
    int //error: not declaring the variable name.
    int x5 //error: not initilizating the variable name with a literal.
}
